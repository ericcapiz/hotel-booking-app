{
  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "incremental": false,
    "composite": false,
    "typeRoots": ["node_modules/@types", "./src/types"]
  },
  "include": ["src"]
}
